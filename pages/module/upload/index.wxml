<!--pages/upload/index.wxml-->
<view class='title'>单图片上传</view>
<view>
  <x-upload urlList='{{url}}' uploadApi='' name='file' bindaddFile='addFile1'></x-upload>
</view>
<view class='title'>多图片上传</view>
<view>
  <x-upload urlList='{{urlList1}}' binddelFile='delFile1' bindaddFile='addFile2' limit='{{limit}}' uploadApi=''></x-upload>
</view>
<view class='title'>多图片上传超过限制自动隐藏添加选择框</view>
<view>
  <x-upload urlList='{{urlList2}}' binddelFile='delFile2'  bindaddFile='addFile3' limit='{{limit}}' uploadApi=''></x-upload>
</view>
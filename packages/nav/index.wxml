<scroll-view scroll-x class="x-class bg-color color15 text-center nav" scroll-with-animation scroll-left="{{scrollLeft}}" enable-flex='true'>
  <view class="item file-item {{index==TabCur?'color6 cur':''}}" wx:for="{{tabList}}" wx:key='nav_key' 
    catchtap='tabSelect' data-index="{{index}}">{{item[key]}}</view>
</scroll-view>

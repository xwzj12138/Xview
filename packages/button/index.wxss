.x-btn{text-align:center;vertical-align:middle;touch-action:manipulation;background-image:none;white-space:nowrap;user-select:none;font-size:14px;border-radius:2px;border:0!important;position:relative;text-decoration:none;box-shadow:inset 0 0 0 1px rgba(0,0,0,.1);color:#fff!important;background:#f7f7f7!important;color:#495060!important;}
.x-btn-hover{opacity:.9}
.x-btn-long{border-radius:0;margin:0;box-shadow:none}
.x-btn-large{height:48px;line-height:48px}
.x-btn-small{height:40px;line-height:40px}
.x-btn-primary{color:#fff!important;background:#2d8cf0!important}
.x-btn-ghost{color:#fff!important;background:#fff!important;color:#495060!important}
.x-btn-success{color:#fff!important;background:#19be6b!important}
.x-btn-warning{color:#fff!important;background:#f90!important}
.x-btn-error{color:#fff!important;background:#ed3f14!important}
.x-btn-info{color:#fff!important;background:#2db7f5!important}
.x-btn-circle{border-radius:44px}
.x-btn-large.x-btn-circle{border-radius:48px}
.x-btn-small.x-btn-circle{border-radius:40px}
.x-btn-loading{opacity:.6}
.x-btn-loading-inner{display:inline-block;margin-right:12px;vertical-align:middle;width:14px;height:14px;background:0 0;border-radius:50%;border:2px solid #fff;border-color:#fff #fff #fff transparent;animation:btn-spin .6s linear;animation-iteration-count:infinite}
.x-btn-disabled{color:#bbbec4!important;background:#f7f7f7!important}
.x-btn-inline{display:inline-block}@keyframes btn-spin{0%{transform:rotate(0)}100%{transform:rotate(360deg)}}